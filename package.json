{
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:full": "tsc -p tsconfig.json && ef-tspm",
    "build:watch": "tsc -w -p tsconfig.json",
    "postbuild": "ef-tspm",

    "discord_bot": "nodemon src/discord --delay 3 -- --client bot",
